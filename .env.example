# RAGVersion Environment Configuration Template
# Copy this file to .env and fill in your credentials

# Supabase Configuration (Required)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key-here

# OpenAI API Key (Required for LangChain)
OPENAI_API_KEY=sk-your-openai-api-key-here

# Optional: Batch Processing Configuration
RAGVERSION_BATCH_MAX_WORKERS=4
RAGVERSION_BATCH_CHUNK_SIZE=100
RAGVERSION_BATCH_ON_ERROR=continue

# Optional: Content Storage Configuration
RAGVERSION_CONTENT_COMPRESSION=gzip
RAGVERSION_CONTENT_TTL_DAYS=365

# Optional: Document Tracking Configuration
RAGVERSION_TRACKING_STORE_CONTENT=true
RAGVERSION_TRACKING_MAX_FILE_SIZE_MB=50
RAGVERSION_TRACKING_HASH_ALGORITHM=sha256

# Optional: Async Configuration
RAGVERSION_ASYNC_ENABLED=true
RAGVERSION_ASYNC_CALLBACK_TIMEOUT=60
RAGVERSION_ASYNC_CALLBACK_MODE=sequential

# Optional: Error Handling Configuration
RAGVERSION_ERROR_CONTINUE_ON_PARSE_ERROR=true
RAGVERSION_ERROR_MAX_RETRIES=3
RAGVERSION_ERROR_RETRY_BACKOFF=exponential
RAGVERSION_ERROR_LOG_ERRORS=true
